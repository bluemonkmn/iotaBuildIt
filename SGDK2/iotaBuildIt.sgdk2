   var tileLeft = leftTile * this.tileset.tileWidth;
   for (var y = topTile; y &lt;= bottomTile; y++) {
      if (rightTile == leftTile) {
         var topMost;
         if (outOfBounds &amp;&amp; ((leftTile &lt; 0) || (leftTile &gt;= this.virtualColumns) || (y &lt; 0) || (y &gt;= this.virtualRows)))
